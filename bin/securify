#!/bin/bash

echo "------------------------------------------" >&2
echo "DEPRECATD: use forkify instead of securify" >&2
echo "------------------------------------------" >&2

if [ -n "$1" ]; then
  forkify "$1" security 'git@lab.civicrm.org:security/'
else
  echo "usage: $0 <add-remotes | set-remotes | fetch>" >&2
  exit 1
fi
