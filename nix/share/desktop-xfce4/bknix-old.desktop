[Desktop Entry]
Version=1.0
Type=Application
Path={{BUILDKIT}}
Exec=xfce4-terminal --active-tab --title 'old#1' -e 'use-bknix old -s' --tab --title 'old#2' -e 'use-bknix old -s' --tab --title 'old#3' -e 'use-bknix old -s'  --tab --title 'old#4' -e 'use-bknix old -s'
Icon=org.xfce.terminalemulator
StartupNotify=true
Terminal=false
OnlyShowIn=XFCE;
X-AppStream-Ignore=True
Name=bknix-old
Comment=Use the command line with old, unsupported PHP/MySQL/etc versions
